{
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Read(~/.zshrc)",

      "Bash(ls:*)",
      "Bash(find:*)",
      "Bash(grep:*)",

      "Bash(bun run:*)",
      "Bash(npm run:*)",
      "Bash(pnpm run:*)",
      "Bash(yarn run:*)"
    ],
    "deny": ["Bash(curl:*)", "Bash(wget:*)"]
  },
  "env": {}
}
